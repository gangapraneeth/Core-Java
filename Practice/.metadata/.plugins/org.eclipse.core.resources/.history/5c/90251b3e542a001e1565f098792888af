package Elements;

public class Eg6 {
	
	//Instance Variables
	int a = 10;
	int b = 20;
	
	//Static Variables
	static int x = 100;
	static int y = 200;

	public static void main(String[] args) {
		
		System.out.println(x);//Instance V calling
		System.out.println(y);
	
		System.out.println(new Eg6().a);
		System.out.println(new Eg6().b);
		
		Eg6 eg6 = new Eg6();
		eg6.m1();
		
		m2();

	}
	
	public void m1() {
		System.out.println(a);
		System.out.println(b);
		System.out.println(Eg6.x);//or (x)
		System.out.println(Eg6.y);
	}

	public static void m2() {
		System.out.println(x);
		System.out.println(y);
	}
}
